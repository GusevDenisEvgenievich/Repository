                <TextBox Text="8" Width="125" Height="3" Margin="0,5,0,0" />
                <TextBox Text="9" Width="125" Height="30" Margin="0,5,0,0" />
                <TextBox Text="10" Width="125" Height="30" Margin="0,5,0,0" />
            </StackPanel>
        </StackPanel>
        <Button Width="125" Height="35" Margin="535,389,0,0" Content="Сохранить" HorizontalAlignment="Left" VerticalAlignment="Top"/>
